QWidget {
    /*border: 0px solid #32414B;*/
    padding: 0px;
    color: #F0F0F0;
    selection-background-color: #1464A0;
    selection-color: #dddddd;
}
QPushButton[text] {
    padding-left: 0.3em; 
} 
QPushButton {
    background-color: qlineargradient(x1: -1, y1: -1, x2: 2, y2: 2,
        stop: 0 #3998ec,
        stop: 1.0 #235fcf);
    border: 1px solid #262626;
    color: #dddddd;
    padding: 3px;
    outline: none;
}
